<Window x:Class="Parcheggio.Views.StatoParcheggio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parcheggio.Views"
        mc:Ignorable="d"
        Title="Stato Parcheggio" Height="255" Width="545.4" Icon="/Views/Icon.ico">
    <Grid>
        <Label FontSize="30" Content="{Binding TitoloPagina}" HorizontalAlignment="Left" Margin="29,10,0,0" VerticalAlignment="Top"/>
        <Label FontSize="25" Content="{Binding VeicoloConTarga}" HorizontalAlignment="Center" Margin="0,95,0,0" VerticalAlignment="Top"/>
        <Button Click="ConfermaClick" FontSize="20" Content="{Binding EntraEsci}" HorizontalAlignment="Center" Margin="0,160,0,0" VerticalAlignment="Top" Width="100" Height="30"/>

    </Grid>
</Window>
